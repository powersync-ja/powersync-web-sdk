{
  "name": "@journeyapps/powersync-web-sdk-monorepo",
  "version": "0.0.0",
  "description": "Monorepo for PowerSync Web SDK",
  "private": true,
  "scripts": {
    "build:all": "pnpm run -r build",
    "build:packages": "pnpm run --filter './packages/**' -r build",
    "clean": "pnpm run -r clean",
    "release": "pnpm build:packages && pnpm changeset publish",
    "watch": "pnpm run --parallel  watch"
  },
  "keywords": [],
  "author": "JOURNEYAPPS",
  "license": "Apache-2.0",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "prettier": "^3.0.3"
  },
  "pnpm": {
    "overrides": {
      "@journeyapps/wa-sqlite": "link:../ja-wa-sqlite"
    }
  }
}
